<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BulletGraph">
        <attr name="noOfFields" format="integer" />
        <attr name="bgColor" format="reference|string|color" />
        <attr name="isWarning" format="boolean" />
        <attr name="title" format="string" />
        <attr name="value" format="integer" />
        <attr name="isASC" format="boolean" />
        <attr name="label_1" />
        <attr name="label_2" />
        <attr name="range" />
    </declare-styleable>
    <declare-styleable name="BulletCircle">
        <attr name="subTitle" format="string" />
    </declare-styleable>
</resources>